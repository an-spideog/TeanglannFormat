{
  "manifest_version": 2,
  "name": "TeanglannFormat",
  "description": "Formats teanglann.ie",
  "version": "0.1.0",
  "icons": {
    "64": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.mozilla.org/*",
        "*://*.teanglann.ie/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "variants.json"
  ]
}
