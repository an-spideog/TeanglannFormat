{
  "manifest_version": 2,
  "name": "TeanglannFormat",
  "description": "Formats teanglann.ie",
  "version": "1.0.1",
  "icons": {
    "64": "icons/tf_logo64.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.teanglann.ie/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "variants.json"
  ],
"browser_specific_settings": {
  "gecko_android": {
    "strict_min_version": "42.0"
  }
}

}
